{
  "manifest_version": 3,
  "name": "Scaler VHR Acceptor",
  "version": "1.0",
  "description": "Automatically accepts the VHRs on TA dashboard.",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
